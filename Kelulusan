import java.util.Scanner;
/**
 *
 * @author Berliani
 */
public class BobotNilaiHuruf {
    public static void main(String[] args){
        System.out.print("Masukkan nilai input = ");
        int nilaiInput = new Scanner(System.in).nextInt();
        String nilai = "";
        
        switch(nilaiInput){
            case 0:
                nilai = "Huruf : E";
                break;
            case 1:
                nilai = "Huruf : D";
                break;
            case 2:
                nilai = "Huruf : C";
                break;
            case 3:
                nilai = "Huruf : B";
                break;
            case 4:
                nilai = "Huruf : A";
                break;
              default:
                nilai = "Nilai yang anda masukkan salah!!!";
                break;  
        }
    System.out.print(nilai);
}   

